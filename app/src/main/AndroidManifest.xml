<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pslproject.testexample">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestExample">
        <activity
            android:name=".View.UI.SearchActivity"
            android:exported="false" />
        <activity
            android:name=".View.UI.UserActivity"
            android:exported="true" />
        <activity
            android:name=".View.UI.RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".View.UI.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".View.UI.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.UI.EditorActivity"
            android:exported="true" />
    </application>

</manifest>